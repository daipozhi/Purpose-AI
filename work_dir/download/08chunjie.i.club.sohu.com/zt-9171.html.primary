var cal_word = function(ele, ele1, max_word)
{
    var text_length = ele.value.length;
    var notice = ele1;
    var left_words = max_word - text_length;
    if(left_words < 0) {
    	ele.value = ele.value.substring(0,max_word-1);
    }
    notice.update('*您还可以输入'+left_words+'字');
};

$('reply_content').observe('keyup' || 'keydown', function(event){
    cal_word($('reply_content'), $('left_words'), 4000);
    if( (event.keyCode == 13) && (event.ctrlKey) ){
       check_submit_form();
    }
});

var check_submit_form = function(){
    club_dialog.showLogin(function(){
        club_notice_float.wait("请稍候", "正在提交，请稍候");
        if(document.new_reply_submit.reply_content.value.length <= 0)
        {
            club_notice_float.error("出错了", '请输入内容后再提交', '', '1');
            return false;
        }
        $('new_reply_submit').submit();
    });
};
