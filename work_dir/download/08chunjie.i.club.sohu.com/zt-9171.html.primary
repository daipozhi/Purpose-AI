function pageGo(num,maxno)
{
	if (typeof(num)!='number')
	{
        if(num == '') return ;
        if (isNaN(num))
        {
        	num = "";
        	return;
        }
        num = parseInt(num);
	}
	
    if (num<1)
    {
        club_notice_float.error("出错了", "请输入大于 0 的整数");
        return;
    }
    
