static	char         m501_l1[SMG_LEN];
static	char         m501_l2[SMG_LEN];
static	char         m501_l3[SMG_LEN];
static	char         m501_s1[SMG_LEN];
static	char	     m501_s2[SMG_LEN];
static	char	     m501_s3[SMG_LEN];

int load6(void)
{
	FILE		*fp1;
    	int         i,j,k;
	//char         l1[SMG_LEN];
	//char         l2[SMG_LEN];
	//char         l3[SMG_LEN];
	//char         s1[SMG_LEN];
	//char	       s2[SMG_LEN];
	char         c1,c2;
	int          pp;

	j=0;
	k=0;

	at6_pp=0;

	fp1=fopen("words-cw02rpt.txt","r");
	if (fp1==NULL)
	{
		MessageBox(0,m501_s1,"message open file error",MB_OK);
		return(1);
	}

	while (!feof(fp1))
	{
		for (i=0;i<SMG_LEN;i++)
		{
			m501_l1[i]=0;
			m501_l2[i]=0;
			m501_l3[i]=0;
		}

		fgets(m501_l1,SMG_LEN,fp1);
	
		k=0;
		pp=0;
		i=0;

		while (i<(int)strlen(m501_l1))
		{
			c1=m501_l1[i+0];
			c2=m501_l1[i+1];

			if (c1<0)
			{
				if (pp==0) // words
				{
					m501_l2[k+0]=c1;
					m501_l2[k+1]=c2;
					m501_l2[k+2]=0;
				}
				else   //repeat times
				{
					m501_l3[k+0]=c1;
					m501_l3[k+1]=c2;
					m501_l3[k+2]=0;
				}
				k=k+2;
  				if (k>=SMG_LEN-3) k=SMG_LEN-3;

				i=i+2;
			}
			else
			{
				if (c1<' ')
				{
					break;
				}
				else
				{
					if (c1==',')
					{
						pp=1;
						k=0;
					}
					else
					{
						if (pp==0)  //words
						{
							m501_l2[k+0]=c1;
							m501_l2[k+1]=0;
						}
						else   // repeat times
						{
							m501_l3[k+0]=c1;
							m501_l3[k+1]=0;
						}
						k++;
  						if (k>=SMG_LEN-3) k=SMG_LEN-3;
					}
				}

				i++;
			}
		}

		if ((int)strlen(m501_l2)>50) continue;

		strcpy(at6[at6_pp],m501_l2);

		at6_n[at6_pp]=str2llint(m501_l3);

		//sprintf(m501_s2,"pp=%d,word=%s,rpt=%lld,",at6_pp,at6[at6_pp],at6_n[at6_pp]);

		//str_gb18030_to_utf8_ini();
		//if (AI_LINUX==1)
		//{
		//	str_gb18030_to_utf8(m501_s2,m501_s3,SMG_LEN);
		//}
		//else
		//{
		//	strcpy(m501_s3,m501_s2);
		//}
		//str_gb18030_to_utf8_close();

		//MessageBox(0,m501_s3,"load5 message",MB_OK);

		at6_pp++;

	}

	fclose(fp1);

	return(0);
}


